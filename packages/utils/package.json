{
  "name": "@tailor-cms/utils",
  "description": "Tailor utility functions",
  "author": "Studion <info@gostudion.com>",
  "license": "MIT",
  "type": "module",
  "version": "1.0.6",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint:fix": "pnpm lint --fix"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "lodash-es": "^4.17.21",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@tailor-cms/interfaces": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "bundledDependencies": [
    "@tailor-cms/interfaces"
  ],
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "target": [
      "node14",
      "es2022"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "bundle": true,
    "minify": false,
    "clean": false,
    "dts": true
  },
  "publishConfig": {
    "access": "public"
  }
}
