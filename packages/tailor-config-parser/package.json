{
  "name": "@tailor-cms/config-parser",
  "description": "Access parsed Tailor config",
  "author": "Studion <tools@gostudion.com>",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "files": ["dist"],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "prebuild": "del-cli dist"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@tailor-cms/interfaces": "workspace:^",
    "del-cli": "^5.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.7.2"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "target": [
      "node14",
      "es2022"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "bundle": true,
    "minify": false,
    "clean": false,
    "dts": true
  },
  "publishConfig": {
    "access": "public"
  }
}
