{
  "name": "@tailor-cms/config",
  "description": "Access parsed Tailor config",
  "author": "ExtensionEngine <info@extensionengine.com>",
  "license": "MIT",
  "version": "1.0.0-beta.25",
  "type": "module",
  "homepage": "https://github.com/ExtensionEngine/tailor#readme",
  "bugs": {
    "url": "https://github.com/ExtensionEngine/tailor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExtensionEngine/tailor.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "prebuild": "del-cli dist",
    "release": "npx np",
    "lint": "eslint --ext .js ."
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@tailor-cms/interfaces": "workspace:^",
    "del-cli": "^5.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.6.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "target": [
      "node14",
      "es2022"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "bundle": true,
    "minify": false,
    "clean": false,
    "dts": true
  },
  "publishConfig": {
    "access": "public"
  }
}
