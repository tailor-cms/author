{
  "name": "@tailor-cms/config",
  "type": "module",
  "version": "2.0.0",
  "description": "Tailor schemas",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "prebuild": "del-cli dist"
  },
  "dependencies": {
    "@tailor-cms/config-parser": "workspace:^"
  },
  "devDependencies": {
    "@tailor-cms/content-container-collection": "workspace:^",
    "@tailor-cms/content-element-collection": "workspace:^",
    "@tailor-cms/interfaces": "workspace:^",
    "@tailor-cms/meta-element-collection": "workspace:^",
    "del-cli": "^6.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@tailor-cms/content-container-collection": "workspace:^",
    "@tailor-cms/content-element-collection": "workspace:^",
    "@tailor-cms/interfaces": "workspace:^",
    "@tailor-cms/meta-element-collection": "workspace:^"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "target": [
      "node14",
      "es2022"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "bundle": true,
    "minify": false,
    "clean": false,
    "dts": true
  }
}
