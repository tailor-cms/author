{
  "name": "tailor-cms",
  "version": "7.0.0",
  "description": "Next-gen Tailor",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "node ./scripts/boot.js",
    "build": "pnpm -r build",
    "start": "cd ./apps/backend && pnpm start",
    "lint": "eslint --ext .js,.ts,.vue .",
    "seed": "cd ./apps/backend && pnpm db:seed",
    "test": "cd tests && pnpm e2e",
    "setup:dev": "pnpm i && node ./scripts/setup/index.js",
    "db:migrate": "cd ./apps/backend && pnpm db migrate",
    "ce:add": "cd ./extensions/content-elements && pnpm ce:add"
  },
  "devDependencies": {
    "@tailor-cms/eslint-config": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "boxen": "^7.1.0",
    "chalk": "^5.3.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": ">=9.0.0",
    "eslint-config-semistandard": ">=17.0.0",
    "eslint-config-standard": ">=17.1.0",
    "eslint-plugin-import": ">=2.28.0",
    "eslint-plugin-n": ">=16.0.1",
    "eslint-plugin-prettier": ">=4.2.1",
    "eslint-plugin-promise": ">=6.1.1",
    "eslint-plugin-vue": ">=9.17.0",
    "eslint-plugin-vuejs-accessibility": ">=2.2.0",
    "execa": "^8.0.1",
    "inquirer": "^9.2.11",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "pg": "^8.11.0",
    "pkg-dir": "^7.0.0",
    "shelljs": "^0.8.5",
    "tailor-seed": "workspace:^",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "fkill": "^8.1.1",
    "pid-port": "^0.2.0"
  }
}
