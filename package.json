{
  "name": "tailor-cms",
  "version": "0.0.1",
  "description": "Next-gen tailor",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "node ./scripts/boot.js",
    "build": "pnpm -r build",
    "start": "cd ./apps/backend && pnpm start",
    "lint": "pnpm -r lint",
    "seed": "cd ./apps/backend && pnpm db:seed",
    "test": "cd tests && pnpm e2e",
    "setup": "node ./scripts/setup/index.js",
    "db:migrate": "cd ./apps/backend && pnpm db migrate",
    "ce:add": "cd ./extensions/content-elements && pnpm ce:add"
  },
  "author": "underscope@gmail.com",
  "license": "GPLv3",
  "devDependencies": {
    "boxen": "^7.1.0",
    "chalk": "^5.3.0",
    "execa": "^8.0.1",
    "inquirer": "^9.2.11",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "pg": "^8.11.0",
    "pkg-dir": "^7.0.0",
    "shelljs": "^0.8.5",
    "tailor-seed": "workspace:^",
    "turbo": "1.3.1"
  },
  "dependencies": {
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "fkill": "^8.1.1",
    "pid-port": "^0.2.0"
  }
}
