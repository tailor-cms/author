{
  "name": "tailor-cms",
  "version": "7.0.0",
  "codename": "X",
  "description": "Next-gen Tailor",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "node ./scripts/boot.js",
    "build": "pnpm -r build",
    "start": "cd ./apps/backend && pnpm start",
    "lint": "eslint --ext .js,.ts,.vue .",
    "seed": "cd ./apps/backend && pnpm db:seed",
    "e2e:functional": "cd tests && pnpm e2e:functional",
    "e2e:visual": "cd tests && pnpm e2e:visual",
    "setup:dev": "pnpm i && node ./scripts/setup/index.js",
    "db:migrate": "cd ./apps/backend && pnpm db migrate",
    "db:reset": "cd ./apps/backend && pnpm db:reset",
    "cc": "cd ./extensions/content-containers && pnpm cli",
    "ce": "cd ./extensions/content-elements && pnpm cli",
    "mi": "cd ./extensions/meta-inputs && pnpm cli"
  },
  "devDependencies": {
    "@tailor-cms/eslint-config": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": ">=9.1.0",
    "eslint-config-semistandard": ">=17.0.0",
    "eslint-config-standard": ">=17.1.0",
    "eslint-plugin-import": ">=2.29.1",
    "eslint-plugin-n": ">=16.6.2",
    "eslint-plugin-prettier": ">=5.1.3",
    "eslint-plugin-promise": ">=6.1.1",
    "eslint-plugin-vue": ">=9.25.0",
    "eslint-plugin-vuejs-accessibility": ">=2.3.0",
    "execa": "^8.0.1",
    "inquirer": "^9.2.19",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "pg": "^8.11.5",
    "pkg-dir": "^7.0.0",
    "shelljs": "^0.8.5",
    "tailor-seed": "workspace:^",
    "typescript": "^5.4.5",
    "vue-eslint-parser": "^9.4.2"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "fkill": "^8.1.1",
    "pid-port": "^0.2.0"
  },
  "packageManager": "pnpm@9.0.6+sha512.f6d863130973207cb7a336d6b439a242a26ac8068077df530d6a86069419853dc1ffe64029ec594a9c505a3a410d19643c870aba6776330f5cfddcf10a9c1617"
}
